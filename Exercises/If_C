#include<stdio.h>

int main(void){
	int numi, numo;
	int a, b;
	int caldi=0;
	
	printf("introduzca un numero \n");
	scanf("%d", &a);
	
	
	if(a%5==0){
		printf("es un multiplo de 5\n");
		
	}
	
	else{
		printf("no es un multiplo de 5\n");
	}
	
printf("\n");

	printf("introduzca un numero \n");
	scanf("%d", &b);
	
	if(b>0){
		printf("Es un numero positivo \n");
	}
	else{
		printf("Es un numero negativo \n");
	}
	
printf("\n");

    printf("introduzca un primer numero \n");
	scanf("%d", &numi);
	
	printf("introduzca un segundo numero \n");
	scanf("%d", &numo );
	
	if(numi==numo){
		printf("son el mismo numero\n");
	}
	else if(numi>numo){
		printf("el primer numero es el mayor \n");
		
	}
	else{
		printf("el segundo numero es el mayor \n");
	}
	
printf("\n");

	printf("introduzca la temperatura de la caldera \n");
	scanf("%d", &caldi);
	
	if(caldi>=50 && caldi<=100){

		printf("Temperatura normal\n");
	}
	else{
		printf("Temperatura fuera de lo normal\n");
	}
	
	
	
	getch();
	return 0;
}
